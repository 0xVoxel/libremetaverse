<?xml version="1.0" ?>
<project name="LibreMetaverse" default="prebuild">
    <property name="project.main.dir" value="${project::get-base-directory()}" />
    
    
    <target name="prebuild">
        <exec program="runprebuild2008.bat" />
    </target>
    
    <target name="build" depends="prebuild">
      <nant buildfile="${project::get-base-directory()}/LibreMetaverse.build" target="build" />
    </target>
    
</project>